{
  "name": "Rosebones",
  "type": "dark",
  "colors": {
    "editor.background": "#1B1826",
    "editor.foreground": "#D0AEAF",
    "editor.lineHighlightBackground": "#222031",
    "editorGroupHeader.tabsBackground": "#242233",
    "editorLineNumber.foreground": "#6B617A",
    "editorLineNumber.activeForeground": "#C4AEB5",
    "activityBar.background": "#1E1E2E",
    "sideBar.background": "#211E2E",
    "sideBarSectionHeader.background": "#3D3854",
    "statusBar.background": "#2A283A",
    "tab.activeForeground": "#E6DFE0",
    "tab.activeBackground": "#1B1826",
    "tab.inactiveBackground": "#312C43",
    "list.activeSelectionBackground": "#242233",
    "list.inactiveSelectionBackground": "#242233",
  },
  "tokenColors": [
    {
      "name": "Comments",
      "scope": ["comment", "punctuation.definition.comment"],
      "settings": {
        "foreground": "#6E6A86",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Keywords",
      "scope": ["keyword", "storage.type", "storage.modifier"],
      "settings": {
        "foreground": "#007693", // #4E8C7C",
        // "fontStyle": "italic"
      }
    },
    {
      "name": "Functions",
      "scope": [
        "entity.name.function",
        "meta.function-call",
        "support.function"
      ],
      "settings": {
        // "foreground": "#4E8C7C"
        "foreground": "#E6DFE0"
      }
    },
    {
      "name": "Variables",
      "scope": ["variable", "meta.definition.variable"],
      "settings": {
        "foreground": "#E6DFE0"
      }
    },
    {
      "name": "Types",
      "scope": ["entity.name.type", "support.type", "storage.type.builtin"],
      "settings": {
        // "foreground": "#4E8C7C"
        "foreground": "#E6DFE0"
      }
    },
    {
      "name": "Strings",
      "scope": ["string", "string.quoted.double", "string.quoted.single"],
      "settings": {
        "foreground": "#C49293"
      }
    },
    // {
    //   "name": "Punctuation",
    //   "scope": [
    //     "punctuation",
    //     "punctuation.definition",
    //     "punctuation.separator",
    //     "punctuation.terminator"
    //   ],
    //   "settings": {
    //     "foreground": "#E6DFE0"
    //   }
    // },
    {
      "name": "Function Parameters",
      "scope": ["variable.parameter", "meta.parameter"],
      "settings": {
        "foreground": "#C4AEB5"
      }
    }
  ]
}
